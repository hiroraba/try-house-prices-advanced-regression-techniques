      "source": [
        "## RSME"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "d1h8dJeO0XAo"
      },
      "source": [
        "def rmse(y_true,y_pred):\n",
        "    #RMSEを算出\n",
        "    rmse = np.sqrt(mean_squared_error(y_true,y_pred))\n",
        "    return rmse"
      ],
      "execution_count": 45,
      "outputs": []
    },
